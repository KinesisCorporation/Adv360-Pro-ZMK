term_new: term_new {
label = "ZM_term_new";
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings = <&macro_press &kp LCTRL &macro_press &kp LALT &macro_tap &kp T>
           , <&macro_release &kp LCTRL &macro_release &kp LALT>
           ;
};
term_new_tab: term_new_tab {
label = "ZM_term_new_tab";
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings = <&macro_press &kp LCTRL &macro_press &kp LSHFT &macro_tap &kp T>
           , <&macro_release &kp LCTRL &macro_release &kp LSHFT>
           ;
};
test: test {
label = "ZM_test";
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings = <&kp T>;
};
